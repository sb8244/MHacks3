<h1>Listing watches</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Selector</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @watches.each do |watch| %>
      <tr>
        <td><%= watch.url %></td>
        <td><%= watch.selector %></td>
        <td><%= link_to 'Destroy', watch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Watch', new_watch_path %>
<%= link_to 'Sign out', destroy_user_session_path, :method => "delete" %>
